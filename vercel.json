{
  "routes": [
    {
      "src": "/api/user(.*)",
      "dest": "/src/routes/user.route.js"
    },
    {
      "src": "/api/products(.*)",
      "dest": "/src/routes/products.route.js"
    },
    {
      "src": "/api/purchases(.*)",
      "dest": "/src/routes/purchase.route.js"
    },
    {
      "src": "/api/create-tables(.*)",
      "dest": "/src/routes/syncup.route.js"
    },
    {
      "src": "/api/details-purchases(.*)",
      "dest": "/src/routes/purchase-details.route.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
